<h2>List of all movies</h1>

<%= Movie.all %>

<h1>List of all directors</h1>

<table border="1">
<% @list_of_directors.each do |a_thing| %>
  <tr>
    <td><%= a_thing.id %></td>
    <td>
      <img src="https://robohash.org/png"<%= a_thing.name %>>
      </td>
    <td><%= a_thing.name %></td>
    <td><%= a_thing.bio %></td>
    <td>
      <a href="/directors/<%= a_thing.id %>" >
      Show details
    </td>

  </tr>
  <% end %>
  </table>
